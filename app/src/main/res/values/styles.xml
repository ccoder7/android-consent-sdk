<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="DialogStyle" parent="Base.Theme.AppCompat.Dialog">
        <item name="colorAccent">#85318F</item>
    </style>

    <style name="ActivityStyle" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorAccent">#85318F</item>
        <item name="cf_textColor">#318F69</item>
        <item name="cf_titleTextColor">#8F5231</item>
        <item name="cf_confirmButtonTextColor">#316E8F</item>
        <item name="cf_backgroundColor">#378F31</item>
        <item name="cf_dividerColor">#8F7831</item>
    </style>

</resources>
